{
  "mailListener": {
    "host": "mailcluster.loopia.se",
    "port": 993,
    "tls": true,
    "connTimeout": 10000,
    "authTimeout": 5000,
    "tlsOptions": { "rejectUnauthorized": false },
    "mailbox": "INBOX",
    "searchFilter": ["UNSEEN"],
    "markSeen": false,
    "fetchUnreadOnStart": true,
    "mailParserOptions": {"streamAttachments": false},
    "attachments": false
  },
  "winston": {
    "logfilePath": "email2photo.log",
    "fileLogLevel": "info",
    "consoleLogLevel": "warn"
  }
}
